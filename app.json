{
  "expo": {
    "name": "GymGirl",
    "description": "a simple workout app",
    "slug": "GymGirl",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffaed7"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.bryanrigsby.gymgirl",
      "config": {
        "googleSignIn": {
          "reservedClientId" : "709728114457-ti1di36e50eolqi06a00cptgqbpqprtr.apps.googleusercontent.com"
        }
      },
      "supportsTablet": true
    },
    "android": {
      "package": "com.bryanrigsby.gymgirl",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
